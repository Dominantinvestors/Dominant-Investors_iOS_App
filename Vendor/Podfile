source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '10.0'

inhibit_all_warnings!
use_frameworks!

project '../project/DominantInvestors'
workspace '../DominantInvestors.xcworkspace'

target :DominantInvestors do
    pod 'ObjectMapper'
    pod 'AlamofireObjectMapper', '~> 5.2'
    pod 'AlamofireImage', '~> 3.5'
    pod 'MBProgressHUD'
    pod 'FacebookCore'
    pod 'FacebookLogin'
    pod 'FacebookShare'
    pod 'AFDateHelper', '~> 4.2.2'
end

target :DominantCrypto do
    pod 'ObjectMapper'
    pod 'AlamofireObjectMapper', '~> 5.2'
    pod 'AlamofireImage', '~> 3.5'
    pod 'MBProgressHUD'
    pod 'FacebookCore'
    pod 'FacebookLogin'
    pod 'FacebookShare'
    pod 'AFDateHelper', '~> 4.2.2'
end

target :Inapps do
    pod 'SwiftyStoreKit'
end
